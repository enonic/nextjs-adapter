{
  "name": "@enonic/nextjs-adapter",
  "version": "2.0.0",
  "license": "Apache-2.0",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean:build": "del-cli dist",
    "clean:test": "del-cli coverage",
    "copy": "npx --yes cpy-cli package.json README.md dist",
    "build": "tsc --build tsconfig.json",
    "lint": "eslint --quiet src",
    "lint:fix": "eslint --fix src",
    "prebuild": "concurrently -c auto -g --timings npm:clean:build npm:test",
    "pretest": "concurrently -c auto -g --timings npm:clean:test npm:lint",
    "test": "jest --no-cache --coverage"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.5.2",
    "html-react-parser": "^5.0.1",
    "lru-cache": "^10.0.1",
    "negotiator": "^0.6.3",
    "next": "^14.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "unescape": "^1.0.1"
  },
  "devDependencies": {
    "@enonic/eslint-config": "^1.1.0",
    "@types/negotiator": "^0.6.3",
    "@types/node": "^20.8.9",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^8.2.2",
    "del-cli": "^5.1.0",
    "eslint": "^8.52.0",
    "eslint-plugin-react": "^7.33.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  }
}
