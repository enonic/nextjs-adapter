{
  "name": "@enonic/nextjs-adapter",
  "version": "2.0.0",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "contributors": [
    "Christian Westgaard <cwe@enonic.com>"
  ],
  "exports": {
    "./guillotine/metadata/*": "./dist/guillotine/metadata/*.js",
    "./guillotine/*": "./dist/guillotine/*.js",
    "./i18n/i18n": "./dist/i18n/i18n.js",
    "./i18n/LocaleContext": "./dist/i18n/LocaleContext.jsx",
    "./query/*": "./dist/query/*.js",
    "./types": "./dist/types/index.d.ts",
    "./utils/*": "./dist/views/*.js",
    "./views/macros/*": "./dist/views/macros/*.jsx",
    "./views/*": "./dist/views/*.jsx",
    "./*": "./dist/*.js",
    ".": "./dist/index.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "enonic",
    "nextjs",
    "adapter",
    "typescript",
    "ecmascript"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "del-cli coverage dist",
    "build": "tsc",
    "build:prod": "concurrently -c auto -g --timings npm:test npm:lint npm:build",
    "madge": "madge -i docs/madge.png -x '(constants|env)\\.ts' src/index.ts",
    "lint": "npx eslint --quiet src",
    "lint:fix": "npx eslint --fix src",
    "prebuild": "del-cli dist",
    "premadge": "npx mkdirp docs",
    "pretest": "del-cli coverage",
    "prepack": "npm run build:prod",
    "test": "jest --no-cache --coverage"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.5.2",
    "gql-query-builder-ts": "^3.9.0",
    "html-react-parser": "^5.0.1",
    "negotiator": "^0.6.3",
    "next": "^14.0.2",
    "q-i": "^2.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "unescape": "^1.0.1"
  },
  "devDependencies": {
    "@edge-runtime/jest-environment": "^2.3.7",
    "@enonic-types/core": "^7.14.0-B2",
    "@enonic/eslint-config": "^1.1.0",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@types/negotiator": "^0.6.3",
    "@types/node": "^20.8.9",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^8.2.2",
    "cpy-cli": "^5.0.0",
    "del-cli": "^5.1.0",
    "eslint": "^8.52.0",
    "eslint-plugin-react": "^7.33.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "madge": "^6.1.0",
    "mkdirp": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  },
  "types": "dist/index.d.ts"
}
